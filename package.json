{
  "name": "vsware-frontend-interview",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"vue-cli-service serve --open\" \"node sever.js\"",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "fake-server": "json-server --watch db.json --port 5000 --middlewares ./middleware.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "concurrently": "^3.5.1",
    "vue": "^2.5.13",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.1",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.1",
    "@vue/cli-service": "^5.0.8",
    "json-server": "^0.12.1",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
